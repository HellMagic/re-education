<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
  <url>
    <loc>http://www.newclass.org/</loc>
    <lastmod><%= @last_modify %></lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.00</priority>
  </url>

  <url>
    <loc>http://www.newclass.org/about-us</loc>
    <lastmod><%= @last_modify %></lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.50</priority>
  </url>

<% Lessonplan.ordered_lessonplans.each do |lessonplan| %>
  <url>
    <loc>http://www.newclass.org<%= lessonplan_path(lessonplan.id) %></loc>
    <lastmod><%= lessonplan.updated_at %></lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.80</priority>
  </url>
<% end %>

</urlset>